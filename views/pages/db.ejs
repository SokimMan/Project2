<!DOCTYPE html>
<html>
<head>
<link rel="stylesheet" href="stylesheets/styles.css">
</head>

<body>

<div class="topnav">
  <a href="home.html">Home</a>
  <a href="searchUser.html">Search for a Contact</a>
  <a class="active" href="/db">View All Contacts</a>
  <a href="newUser.html">Create New Contact</a>
</div>

<h2>Contacts Results</h2>

<table>
  <tr>
    <th> ID</th>
    <th> First Name</th>
    <th> Last Name</th>
    <th> City</th>
  </tr>

<% results.forEach(function(r) { %>
	<tr>
    <td><%= r.external_id %></td>
    <td><%= r.firstname %></td>
    <td><%= r.lastname %></td>
    <td><%= r.city %></td>
  	</tr>
<% }); %>

</table>
<br>
<form id= "back" action="/home.html" method="get">
	<input type="submit" value="Go Back">




</body>
</html>
